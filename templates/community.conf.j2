        {{ community_index_group_inet }}:{{ community_index_pre1 }}
        {{ community_index_group_inet }}:{{ community_index_pre2 }}
        {{ community_index_group_inet }}:{{ community_index_pre3 }}
        {{ community_index_group_inet }}:{{ community_index_pre4 }}

        {{ community_index_group_peer }}:{{ community_index_bh }}
        {{ community_index_group_peer }}:{{ community_index_dna }}
        {{ community_index_group_inet }}:{{ community_index_pre1 }}
        {{ community_index_group_inet }}:{{ community_index_pre2 }}
        {{ community_index_group_inet }}:{{ community_index_pre3 }}
        {{ community_index_group_inet }}:{{ community_index_pre4 }}

        {{ origin_asn }}:{{ community_index_bh }}
        {{ origin_asn }}:{{ community_index_dna }}
        {{ origin_asn }}:{{ community_index_pre1 }}
        {{ origin_asn }}:{{ community_index_pre2 }}
        {{ origin_asn }}:{{ community_index_pre3 }}
        {{ origin_asn }}:{{ community_index_pre4 }}
        ];
    }

    replace: community from-inet        members {{ origin_asn }}:{{ community_index_group_inet }};
    replace: community from-ix          members {{ origin_asn }}:{{ community_index_group_ix }};
    replace: community from-peer        members {{ origin_asn }}:{{ community_index_group_peer }};
    replace: community from-customer    members {{ origin_asn }}:{{ community_index_group_customer }};

    replace: community dna-all  members {{ origin_asn }}:{{ community_index_dna }};
    replace: community dna-inet members {{ community_index_group_inet }}:{{ community_index_dna }};
    replace: community dna-ix   members {{ community_index_group_ix }}:{{ community_index_dna }};
    replace: community dna-peer members {{ community_index_group_peer }}:{{ community_index_dna }};

    replace: community prepend-all-1 members {{ origin_asn }}:{{ community_index_pre1 }};
    replace: community prepend-all-2 members {{ origin_asn }}:{{ community_index_pre2 }};
    replace: community prepend-all-3 members {{ origin_asn }}:{{ community_index_pre3 }};
    replace: community prepend-all-4 members {{ origin_asn }}:{{ community_index_pre4 }};

    replace: community prepend-inet-1 members {{ community_index_group_inet }}:{{ community_index_pre1 }};
    replace: community prepend-inet-2 members {{ community_index_group_inet }}:{{ community_index_pre2 }};
    replace: community prepend-inet-3 members {{ community_index_group_inet }}:{{ community_index_pre3 }};
    replace: community prepend-inet-4 members {{ community_index_group_inet }}:{{ community_index_pre4 }};

