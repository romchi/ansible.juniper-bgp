    replace: community prepend-peer-1 members {{ community_index_group_peer }}:{{ community_index_pre1 }};
    replace: community prepend-peer-2 members {{ community_index_group_peer }}:{{ community_index_pre2 }};
    replace: community prepend-peer-3 members {{ community_index_group_peer }}:{{ community_index_pre3 }};
