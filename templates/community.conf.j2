policy-options {

    replace: community {{ origin_name }}-origin members {{ origin_asn }}:{{ origin_asn }};
    replace: community blackhole-all members {{ origin_asn }}:{{ community_index_bh }};

    community for-blackhole members {{ origin_asn }}:{{ community_index_bh }};

    community unsupported-communities {
    invert-match;
    members [
        {{ community_index_group_inet }}:{{ community_index_bh }}
        {{ community_index_group_inet }}:{{ community_index_dna }}
        {{ community_index_group_inet }}:{{ community_index_pre1 }}
        {{ community_index_group_inet }}:{{ community_index_pre2 }}
        {{ community_index_group_inet }}:{{ community_index_pre3 }}
        {{ community_index_group_inet }}:{{ community_index_pre4 }}

        {{ community_index_group_ix }}:{{ community_index_bh }}
